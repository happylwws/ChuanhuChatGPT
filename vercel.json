{
  "version": 2,
  "builds": [
    {
      "src": "ChuanhuChatbot.py",
      "use": "@vercel/python",
      "config": { 
        "runtime": "python3.10" ,
        "postinstall": "apt-get update && apt-get install -y swig" 
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "ChuanhuChatbot.py"
    }
  ]
}
